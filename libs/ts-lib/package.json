{
  "name": "@activate/api-models",
  "version": "1.0.0",
  "description": "Generated DTO models and API for TypeScript/React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf dist src",
    "copy": "npm run copy:models && npm run copy:api",
    "copy:models": "mkdir -p src && cp -r ../../gen/ts-models/* src/",
    "copy:api": "mkdir -p src/api && cp -r ../../gen/react-api/* src/api/",
    "prebuild": "npm run clean && npm run copy",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "dto",
    "typescript",
    "react",
    "api"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "author": "",
  "license": "MIT"
}
